{
	"name": "floating-ui-menu",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"stylelint": "stylelint \"src/**/*.{css,scss}\"",
		"storybook": "NODE_PATH=$(pwd)/node_modules storybook dev --port 6018 --ci",
		"build-storybook": "NODE_PATH=$(pwd)/node_modules storybook build",
		"preview-storybook": "http-server storybook-static --port 6018 --silent",
		"test": "wait-on tcp:6018 && test-storybook --url 'http://localhost:6018'",
		"test-ci": "run-p --race test preview-storybook"
	},
