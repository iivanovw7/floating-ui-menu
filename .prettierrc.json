{
	"arrowParens": "always",
	"singleQuote": false,
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"trailingComma": "es5",
	"importOrder": ["^react.*$", "^[^(.|alias)].*$", "^alias/.*$", "^(../|./)(?!.*.scss$).*", ".scss$"],
	"importOrderSeparation": true,
	"importOrderParserPlugins": ["jsx", "typescript", "scss"]
}
